<div class= "login">
<%= form_tag "/login" do %>
  <%= label_tag :email %><br>
  <%= text_field_tag :email %><br>
  <%= label_tag :password %><br>
  <%= password_field_tag :password %><br>
  <%= submit_tag "Login" %>
<% end %> 
  <%= link_to('Log in with Facebook!', '/auth/facebook')%> 
  <%= link_to('Log in with Google!', '/auth/google_oauth2')%> 
</div>